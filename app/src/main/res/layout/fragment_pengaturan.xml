<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.biodata.BiodataFragment">

    <include
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_profil"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="164dp"
        android:layout_height="102dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="192dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/faq2"
        app:layout_constraintEnd_toStartOf="@+id/imageButton3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="164dp"
        android:layout_height="102dp"
        android:layout_marginTop="192dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/faq1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button2"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        app:strokeColor="#DC143C"
        app:strokeWidth="1.5dp"
        android:layout_width="344dp"
        android:layout_height="60dp"
        android:layout_marginTop="160dp"
        android:text="Frequently Asked Question"
        android:drawableEnd="@drawable/dropdown"
        android:textColor="@color/red"
        android:backgroundTint="@color/white"
        android:textAllCaps="false"
        android:textAlignment="textStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

    <Button
        android:id="@+id/gantiPass"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        app:strokeColor="#DC143C"
        app:strokeWidth="1.5dp"
        android:layout_width="344dp"
        android:layout_height="60dp"
        android:layout_marginTop="300dp"
        android:text="Ganti Password"
        android:drawableEnd="@drawable/pass"
        android:textColor="@color/red"
        android:backgroundTint="@color/white"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

    <Button
        android:id="@+id/button3"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        app:strokeColor="#DC143C"
        app:strokeWidth="1.5dp"
        android:layout_width="344dp"
        android:layout_height="60dp"
        android:layout_marginTop="230dp"
        android:text="Cara Memanggil Ambulance"
        android:drawableEnd="@drawable/dropdown"
        android:textColor="@color/red"
        android:backgroundTint="@color/white"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

    <Button
        android:id="@+id/signOutBtn"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        app:strokeColor="#DC143C"
        app:strokeWidth="1.5dp"
        android:layout_width="344dp"
        android:layout_height="60dp"
        android:layout_marginTop="370dp"
        android:text="Keluar"
        android:textColor="@color/red"
        android:backgroundTint="@color/white"
        android:drawableEnd="@drawable/logout"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

</androidx.constraintlayout.widget.ConstraintLayout>
